<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>QuestionServlet</servlet-name>
        <servlet-class>net.aiog.web.QuestionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ContestServlet</servlet-name>
        <servlet-class>net.aiog.web.ContestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QuestionServlet</servlet-name>
        <url-pattern>/qs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuestionServlet</servlet-name>
        <url-pattern>/question</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContestServlet</servlet-name>
        <url-pattern>/cs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContestServlet</servlet-name>
        <url-pattern>/contest</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
